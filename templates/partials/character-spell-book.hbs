<h1>TEST</h3>



    {{#ifCond spells.level1.length '>' 0}}
    <button>sort level 1</button>
    <table>
        <tr>
            <th></th>
            <th>Ecole</th>
            <th>Nom</th>
            <th>Niveau</th>
            <th>Cout</th>
            <th></th>
            <th></th>
        </tr>
        {{#each spells.level1 as |spell id|}}
        {{>"systems/naheulbeuk/templates/cards/spell-book-card.hbs" spell=spell}}
        {{/each}}
    </table>
    {{/ifCond}}

